<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :artist_names %>
  <%= f.text_field :artist_name %><br>
  <%= f.collection_select :genre_id,Genre.all, :id, :name %> <br>
  Song Notes 1:<textarea name="song[note_content][]" id="song_notes_1"></textarea><br>
  Song Notes 2:<textarea name="song[note_content][]" id="song_notes_2"></textarea><br>

 
  <br>

  <%= f.submit %>
<% end %>


